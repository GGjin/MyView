<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BrokenLineView">
        <attr name="main_line_color" format="color"/>
        <attr name="mode" format="integer">
            <enum name="line_with_point" value="0"/>
            <enum name="line_without_point" value="1"/>
            <enum name="broken_line_with_point" value="2"/>
            <enum name="broken_line_without_point" value="3"/>
        </attr>
    </declare-styleable>
</resources>